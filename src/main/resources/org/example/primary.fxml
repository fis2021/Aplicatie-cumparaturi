<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>

<VBox alignment="CENTER" prefHeight="292.0" prefWidth="396.0" spacing="20.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.controllers.PrimaryController">
   <children>
      <ImageView fx:id="cartView" fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../../../Images/cart.png" />
         </image>
      </ImageView>
      <Label text="Login">
         <font>
            <Font size="23.0" />
         </font>
      </Label>
      <TextField promptText="Username" fx:id="username"/>
      <PasswordField prefHeight="31.0" prefWidth="84.0" promptText="Password" fx:id="password"/>
      <Button fx:id="primaryButton" onAction="#switchToSecondary" text="Nu ai cont?" />
      <Button mnemonicParsing="false" text="Login" onAction="#handleLoginAction" />
      <Text fx:id="message" strokeType="OUTSIDE" strokeWidth="0.0" text="" />
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
